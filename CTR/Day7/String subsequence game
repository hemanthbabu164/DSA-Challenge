Q)Given a string return all unique possible subsequences which start with vowel and end with consonant. 
A String is a subsequence of a given String, that is generated by deleting some character of a given string without changing its order.
NOTE: Return all the unique subsequences in lexicographically sorted order. 

//code

//{ Driver Code Starts
// Initial Template for C++
#include <bits/stdc++.h>
using namespace std;

// } Driver Code Ends
// User function Template for C++
class Solution {
  public:
    set<string> allPossibleSubsequences(string S) {
        // code here
        int k=S.length();
        set<string> st;
        
        for(int i=0;i<pow(2,k);i++)
        {
            string a="";
            for(int j=0;j<k;j++)
            {
                if(i & (1<<j))
                {
                    a+=S[j];
                }
            }
            
            if(a.length()>1)
            {
                
            char c1=a[0];
            char c2=a[a.length()-1];
            
            if((c1=='a'||c1=='e'||c1=='i'||c1=='o'||c1=='u') && (c2!='a'&&c2!='e'&&c2!='i'&&c2!='o'&&c2!='u'))
            {
                st.insert(a);
            }
            
            }
        }
        return st;
    }
};

//{ Driver Code Starts.
int main() {
    int t;
    cin >> t;
    while (t--) {
        string S;
        cin >> S;
        Solution ob;
        set<string> ans = ob.allPossibleSubsequences(S);
        if (ans.size() == 0)
            cout << -1 << endl;
        else {
            for (auto i : ans) cout << i << " ";
            cout << endl;
        }
    }
    return 0;
}

// } Driver Code Ends
